  "name": "Iris Flywheel Compat",
  "description": "Allow Flywheel instancing optimization to be enabled when using iris.",
  "authors": [
    "Leon"
  ],
  "contact": {
    "sources": "https://github.com/leon-o/iris-flw-compat"
  },
  "license": "CC0-1.0",
  "icon": "assets/irisflw/icon.png",
  "environment": "client",
  "entrypoints": {
