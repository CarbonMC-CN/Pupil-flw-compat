    "irisflw.mixins.flw.json",
    "irisflw.mixins.iris.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*",
    "iris": ">=1.8.1",
    "flywheel": ">=1.0.0"
